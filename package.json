{
  "name": "@mastodonus/test-vue-library",
  "description" : "",
  "author": "Adam Osmond (@mastodonus)",
  "homepage": "https://github.com/mastodonus/test-vue-package#readme",
  "version": "0.0.2",
  "main": "./dist/test-vue-library.umd.cjs",
  "module": "./dist/test-vue-library.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/test-vue-library.js",
      "require": "./dist/test-vue-library.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastodonus/test-vue-package.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "publishConfig": {
    "@mastodonus:registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/mastodonus/test-vue-package/issues"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  }
}
